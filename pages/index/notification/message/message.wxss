/* pages/index/notification/message/message.wxss */
page{
  background-color: #F5F5FA;
  width: 100%;
  height: 100%;
}
.msgbox{
  width: 638rpx;
  padding: 28rpx;
  min-height: 190rpx;
  background: #fff;
  border:1rpx solid rgba(235,235,235,1);
  border-radius:10rpx;
  margin-left: 28rpx;
  margin-top: 21rpx;
}
.msgtitle{
  color: #191919;
  font-size: 30rpx
}
.reddot {
  width: 12rpx;
  height: 12rpx;
  background: red;
  border-radius: 50%;
  display: inline-block;
  margin-right: 11rpx;
  margin-bottom: 6rpx;
}
.msgdate{
  color: #969696;
  font-size: 24rpx;
  margin-top: 18rpx;
  margin-bottom: 36rpx;
}
.msgcontent{
  color: #555;
  font-size: 26rpx;
}