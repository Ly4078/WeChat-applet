page {
  height: 100%;
  background-color: #f5f5fa;
}
.flashSaleHome {
  /* min-height: 100%; */
  background-color: #78b6fc;
}

.flashSaleHome.active {
  background-color: #f5f5fa;
}

.flashSaleWrap {
  width: 100%;
  height: 250rpx;
  display: flex;
  justify-content: space-between;
  position: relative;
  margin: 20rpx 0;
  background-color: #fff;
  border-radius: 16rpx;
}

.flashSale-left {
  width: 250rpx;
  height: 250rpx;
}

.flashSale-img {
  width: 250rpx;
  height: 250rpx;
  border-top-left-radius: 16rpx;
  border-bottom-left-radius: 16rpx;
}

.flashSale-right {
  width: 472rpx;
  height: auto;
  padding-left: 26rpx;
  background-color: #fff;
  border-top-right-radius: 16rpx;
  border-bottom-right-radius: 16rpx;
}

.flashSaleBut {
  width: 250rpx;
  height: 66rpx;
  position: absolute;
  bottom: -1rpx;
  left: 0rpx;
  z-index: 999;
}

.flashSaleRig-row {
  padding-top: 14rpx;
  width: 90%;
}

.diningName {
  width: 96%;
  font-size: 36rpx;
  font-weight: 500;
  color: rgba(252, 94, 45, 1);
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}

.diningNameImg {
  width: 20rpx;
  height: 20rpx;
  vertical-align: middle;
}

.sumName text:nth-child(2) {
  font-size: 20rpx;
  color: rgba(177, 177, 177, 1);
  vertical-align: middle;
  padding: 0rpx 30rpx 0rpx 8rpx;
}

.sumName text:nth-child(3) {
  font-size: 20rpx;
  color: rgba(177, 177, 177, 1);
}

.sumPurchase {
  margin-top: 28rpx;
  width: 100%;
  display: flex;
  justify-content: space-between;
}

.sumResidue {
  width: 50%;
}

.sumResidue progress {
  border-radius: 50rpx;
}

.oncePurchase {
  width: 40%;
  display: flex;
  align-items: flex-end;
}

.oncePurchase button {
  width: 140rpx;
  height: 74rpx;
  line-height: 74rpx;
  color: #fff;
  font-size: 30rpx;
  background-color: #fc5e2d;
}

.fixation {
  font-size: 40rpx;
  font-weight: 600;
  color: rgba(252, 94, 45, 1);
}

.fixation text:nth-child(1) {
  font-size: 24rpx;
  color: rgba(252, 94, 45, 1);
}

.fixationTop text:nth-child(2) {
  font-size: 24rpx;
  font-family: PingFangSC-Regular;
  font-weight: 400;
  text-decoration: line-through;
  color: rgba(177, 177, 177, 1);
  padding-left: 18rpx;
}

.mySeckillWrap {
  padding: 20rpx 28rpx;
  margin-bottom: 20rpx;
  box-sizing: border-box;
  width: 100%;
  display: flex;
  justify-content: space-between;
  background-color: #fff;
}

.mySeckill_l {
  display: flex;
  flex-direction: row;
}

.mySeckill_l image {
  height: 120rpx;
  width: 120rpx;
  border-radius: 8rpx;
}

.mySeckill_info {
  font-size: 24rpx;
  margin-left: 26rpx;
}

.secKill_name {
  font-size: 30rpx;
  color: #191919;
  width: 400rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}

.secKill_people {
  color: #fc5e2d;
  padding: 6rpx 0 8rpx 0;
}

.secKill_time {
  color: #2d2a2a;
}

.secKill_time .voteText {
  display: inline-block;
  background-color: #2d2a2a;
  color: #fff;
  border-radius: 4rpx;
  padding: 0 4rpx;
  margin-right: 6rpx;
}

.mySeckill_r {
  display: flex;
  align-items: center;
}

.mySeckill_r button {
  width: 132rpx;
  height: 60rpx;
  line-height: 60rpx;
  font-size: 24rpx;
  color: #fff;
  border-radius: 10rpx;
  background-color: #fc5e2d;
}

/* 进度条样式 */

.accuracyQay {
  width: 186rpx;
  height: 22rpx;
  background: rgba(255, 175, 150, 1);
  border-radius: 11rpx;
  position: relative;
  margin-top: 8rpx;
}

.accuracyQay-text {
  position: absolute;
  left: 56rpx;
  top: -2rpx;
  font-size: 20rpx;
  color: rgba(255, 255, 255, 1);
}

.actualaccurac {
  /* width: 126rpx; */
  height: 22rpx;
  background: rgba(252, 94, 45, 1);
  border-radius: 10rpx;
}
