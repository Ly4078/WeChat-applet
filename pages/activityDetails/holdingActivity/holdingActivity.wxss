page{
  background-color: #f8803e;
}

.holdingImg{
  width: 100%;
  height: 650rpx;
}

.holFlow{
  width: 100%;
  height: 100%;
  display: flex;
  justify-content: center;
}

.holFlowCtn{
  width: 92%;
}

.holdingStep{
  width: 100%;
  height: 188rpx;
}

.holFlowTop{
  width: 100%;
  height: 360rpx;
  display: flex;
  justify-content: space-between;
}

.process{
  width:336rpx;
  height:360rpx;
  background:rgba(255,255,255,1);
  border-radius:16rpx;
  box-shadow:0rpx 10rpx 16rpx 0rpx rgba(63,59,149,0.6);
}

.amount{
  width: 100%;
  height: 68rpx;
  background:rgba(243,243,243,1);
  border-radius:16rpx;
  text-align: center;
  letter-spacing: 2rpx;
}

.amount text{
  font-size:30rpx;
  color:rgba(87,87,87,1);
  line-height:32px;
  font-weight: 600;
}

.amountDig{
  width: 100%;
  height: 184rpx;
  font-size:30rpx;
  color:rgba(252,60,39,1);
  text-align: center;
  line-height: 184rpx;
}

.amountDigText{
  font-size: 82rpx;
}

.invite{
  width: 100%;
  height: 108rpx;
  display: flex;
  letter-spacing: 2rpx;
  justify-content: center;
}

.invite button{
  width:200rpx;
  height:78rpx;
  line-height:78rpx;
  border: 0;
  background:linear-gradient(0deg,rgba(251,50,26,1) 0%,rgba(255,107,101,1) 100%);
  border-radius:39rpx;
  box-shadow:0rpx 10rpx 16rpx 0rpx rgba(214,25,4,0.33);
  font-size:32rpx;
  color: #ffffff;
}

.market{
  width:100%;
  height:206rpx;
  background:rgba(255,255,255,1);
  border-radius:16rpx;
  box-shadow:0rpx 10rpx 16rpx 0rpx rgba(213,96,32,0.6);
  margin-top: 30rpx;
}

.marketTop{
  width: 100%;
  height: 68rpx;
  background:rgba(243,243,243,1);
  border-radius:16rpx;
  text-align: center;
  line-height: 68rpx;
}

.marketTop text{
  font-size:30rpx;
  color:rgba(87,87,87,1);
  line-height:32rpx;
}

.markdistance{
  width: 100%;
  height: 138rpx;
  display: flex;
  justify-content: center;
  overflow: hidden;
  border-bottom-left-radius: 14rpx;
  border-bottom-right-radius: 14rpx;
}

.single{
  width:176rpx;
  height:138rpx;
  background:rgba(25,25,25,1);
  opacity:0.9;
  position: relative;
  z-index: 9999;
  /* border-bottom-left-radius: 16rpx; */
}

.single.active {
  background:#ff6c24;
  opacity:1;
}

.illegality{
  width:25%;
  height:138rpx;
  background:rgba(25,25,25,1);
  opacity:0.7;
  position: relative;
}

.single:nth-child(1){
  border-bottom-left-radius: 16rpx;
}

.single:nth-child(5){
  border-bottom-right-radius: 16rpx;
}

.detail{
  width: 100%;
  height: 90rpx;
  font-size: 24rpx;
  color:rgba(255,255,255,1);
  text-align: center;
}

.detail view:nth-child(1){
  padding-top: 20rpx;
}

.small{
  width: 100%;
  height: 48rpx;
  text-align: center;
}

.smallImage{
  width: 26rpx;
  height: 26rpx;
}

.settingimg{
  width: 176rpx;
  height: 138rpx;
  position: absolute;
  top: 0rpx;
  left: 0rpx;
  z-index: 9;
}

.regulation{
  width: 100%;
  height: 120rpx;
  text-align: center;
  line-height: 120rpx;
  padding-top: 20rpx;
}

.regulation view{
  color: #ffffff;
}

.regulationText{
  padding: 0rpx 24rpx;
  font-size:28rpx;
}

.parcel{
  width: 100%;
  font-size: 24rpx;
  color:rgba(255,255,255,1);
  display: flex;
  justify-content: space-between;
  padding-top: 26rpx;
}

.parcelText{
  width: 95%;
  letter-spacing: 2rpx;
}

.parcelText{
  text-align: left;
}

.parcel_top{
  width: 95%;
  letter-spacing: 2rpx;
}

.parcel:nth-child(1){
  padding-top: 0rpx!important;
  border: 1px solid red;
}

.parcel:last-child{
  padding-bottom: 60rpx;
}

.indexIcon {
  position: fixed;
  right: 20rpx;
  bottom: 100rpx;
  z-index: 1000;
  width: 100rpx;
  height: 100rpx;
}