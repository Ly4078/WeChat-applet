page{
  display: flex;
  flex-direction: column;
  height: 100%;
  background-color: #F5F5FA;
}
.vouchertop {
  width: 694rpx;
  height: 370rpx;
  margin: 15rpx 28rpx;
  background-image: url(https://xqmp4-1256079679.file.myqcloud.com/test_quanbj.png);
  background-repeat: no-repeat;
  background-size: 100% 100%;
  -moz-background-size: 100% 100%;
  position: relative;
}

.vouchername {
  float: right;
  margin-top: 33%;
  font-size: 24rpx;
  font-family: PingFangSC-Regular;
  font-weight: 400;
  color: rgba(255, 255, 255, 1);
  line-height: 40rpx;
  margin-right: 30rpx;
}

.submitTop {
  width: 100%;
  height: 126rpx;
  background-color: #fff;
  display: flex;
  justify-content: center;
  margin: 20rpx 0;
}

.submitCenter {
  width: 92%;
  height: 126rpx;
  display: flex;
  justify-content: space-between;
  align-items: center;
}

.AddTheAddress {
  width: 40rpx;
  height: 40rpx;
  vertical-align: middle;
  margin-top: 6rpx;
}

.submitLeft {
  width: 86%;
  height: auto;
}

.submitLeft text {
  font-size: 30rpx;
  letter-spacing: 2rpx;
  vertical-align: middle;
}

.compellation {
  font-size: 30rpx;
  font-family: PingFangSC-Regular;
  color: rgba(25, 25, 25, 1);
  display: flex;
  align-items: center;
}

.compellation text {
  padding-left: 46rpx;
}

.compellationText {
  font-size: 24rpx;
  color: rgba(128, 128, 128, 1);
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
  padding-top: 18rpx;
}

.submitRight {
  width: 22rpx;
  height: 12rpx;
  transform: rotate(-90deg);
  vertical-align: middle;
}

.indentDetails {
  width: 100%;
  display: flex;
  justify-content: center;
  background-color: #fff;
}

.indentDetailsCtn {
  width: 92%;
  height: auto;
}

.indentDetailsTop {
  width: 100%;
  height: 176rpx;
  display: flex;
  justify-content: space-between;
  border-bottom: 1px solid #ebebeb;
}

.indentDetailsLeft {
  width: 20%;
  height: auto;
  display: flex;
  justify-content: center;
  align-items: center;
}

.indentDetailsRight {
  width: 80%;
  height: auto;
  letter-spacing: 2rpx;
  margin-left: 26rpx;
}

.indentDetailsImg {
  width: 120rpx;
  height: 120rpx;
  background: rgba(255, 255, 255, 1);
  border: 1rpx solid rgba(235, 235, 235, 1);
  border-radius: 6rpx;
}

.equally {
  width: 100%;
  height: 88rpx;
  border-bottom: 1px solid #ebebeb;
  display: flex;
  justify-content: space-between;
  align-items: center;
  font-size: 28rpx;
  color: rgba(128, 128, 128, 1);
  letter-spacing: 2rpx;
}

.equally input {
  width: 75%;
}

.equally:last-child {
  border-bottom: 0rpx;
}

.eqpicker {
  width: 100%;
  height: 88rpx;
}

.picker {
  width: 100%;
  height: 88rpx;
  line-height: 88rpx;
  display: flex;
  justify-content: space-between;
  align-items: center;
}

.intervalBind {
  font-size: 26rpx;
}

.datenum {
  float: right;
}

.delicatese {
  color: #fc5e2d;
}

.voubut{
  width: 100%;
  height: 88rpx;
  position: fixed;
  left: 0;
  bottom: 0;
}
.voubut view{
  width: 50%;
  float: left;
  height: 88rpx;
  line-height: 88rpx;
  text-align: center;
  font-size: 28rpx;
}
.leftbut{
  background: #fff;
  color: #191919;
}
.rightbut{
  background:#FC5E2D;
  color: #fff;
}