.order-for-shop {
  width: 100%;
  height: 122rpx;
  background-color: #fff;
  display: flex;
  justify-content: center;
  margin-bottom: 20rpx;
}

.order-for-goods {
  width: 100%;
  background-color: white;
  border-top: 1rpx dotted #e0e0e0;
  position: relative;
  padding: 20rpx 36rpx 0 32rpx;
  box-sizing: border-box;
}

.order-for-goods::before, .order-for-goods::after {
  content: '';
  width: 30rpx;
  height: 30rpx;
  border-radius: 50%;
  position: absolute;
  background-color: #f5f5fa;
  top: -15rpx;
}

.order-for-goods::before {
  left: -12rpx;
}

.order-for-goods::after {
  right: -12rpx;
}

.order-for-nei {
  width: 100%;
  padding-bottom: 30rpx;
