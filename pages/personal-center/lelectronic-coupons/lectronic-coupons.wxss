  margin-bottom: 20rpx;
}

.lectronic-top {
  width: 100%;
  height: 168rpx;
  display: flex;
  justify-content: space-between;
  border: 1px dotted #e0e0e0;
  box-sizing: border-box;
}

.lectronic-left {
  width: 80%;
  height: 168rpx;
  display: flex;
  justify-content: center;
  align-items: center;
}

.lectronic-right {
  width: 20%;
  height: 168rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  color: #555;
  font-size: 28rpx;
  letter-spacing: 1rpx;
}

.lectronic-images {
  width: 102rpx;
  height: 100rpx;
}

.lectronic-wrap-im {
  width: 24%;
  height: 168rpx;
  display: flex;
  justify-content: center;
  align-items: center;
}

.lectronic-text {
  width: 74%;
  height: 120rpx;
  letter-spacing: 2rpx;
}

.lectronic-reel {
  display: block;
  color: #191919;
  font-size: 46rpx;
  padding-top: 30rpx;
}

.lectronic-limit {
  color: #555;
  font-size: 30rpx;
}

.restrict-time {
  width: 100%;
  height: 62rpx;
  color: #b1b1b1;
  font-size: 24rpx;
  display: flex;
  justify-content: space-between;
  align-items: center;
}

.restrict-Images {
  width: 12rpx;
  height: 22rpx;
  vertical-align: middle;
}

.restrict-caution {
  padding-left: 30rpx;
}

.restric-page-down {
  padding-right: 30rpx;
}

.restric-page-text {
  padding-right: 15rpx;
  vertical-align: middle;
}

.qr-code {
  width: 100%;
  background-color: white;
  margin-bottom: 20rpx;
}

.rollMessage {
  height: 60rpx;
  width: 100%;
  border-bottom: 1px dashed #e0e0e0;
  font-size: 24rpx;
  color: #b1b1b1;
  text-indent: 30rpx;
  line-height: 60rpx;
}

.coding-code {
  width: 100%;
  height: 90rpx;
  border-bottom: 1px solid #e0e0e0;
  font-size: 32rpx;
  color: #555;
  text-align: center;
  align-items: center;
  letter-spacing: 2px;
  position: relative;
  top: 18rpx;
}

.coding-code.show {
  display: block;
}

.coding-code.hide {
  display: none;
}

.codingText {
  padding-left: 30rpx;
}

.numberFeald {
