page {
  background-color: #ececec;
  width: 100%;
  height: 100%;
}

.lectronicBox {
  width: 100%;
  height: 100%;
  padding: 32rpx 28rpx;
  box-sizing: border-box;
  background-color: #f5f5fa;
}

.lectronic-wrap {
  width: 100%;
  /* min-height: 168rpx; *//* background-color: white; *//* margin-bottom: 20rpx; */
}

.lectronic-top {
  width: 100%;
  height: 70rpx;
  line-height: 70rpx;
  background-color: #fff;
  color: #000;
  border-bottom: 1rpx solid #e0e0e0;
  display: flex;
  justify-content: space-between;
  font-size: 30rpx;
  padding: 0 30rpx 0 24rpx;
  box-sizing: border-box;
  letter-spacing: 4rpx;
}

.lectronic-left {
  height: 100%;
}

.lectronic-right {
  height: 100%;
  color: red;
}

.lectronic-images {
  width: 102rpx;
  height: 100rpx;
}

.lectronic-wrap-im {
  width: 24%;
  min-height: 108rpx;
  display: flex;
  justify-content: center;
  align-items: center;
}

.success {
  width: 100%;
  height: 100%;
  color: #4cba1b;
  font-size: 40rpx;
  text-align: center;
  line-height: 232rpx;
}

.lectronic-text {
  height: 120rpx;
  letter-spacing: 2rpx;
}

.lectronic-limit {
  color: #555;
  font-size: 30rpx;
}

.restrict-time {
  width: 100%;
  height: 62rpx;
  color: #b1b1b1;
  font-size: 24rpx;
  display: flex;
  justify-content: space-between;
  align-items: center;
}

.restrict-Images {
  width: 12rpx;
  height: 22rpx;
  vertical-align: middle;
}

.restrict-caution {
  padding-left: 30rpx;
}

.restric-page-down {
  padding-right: 30rpx;
}

.restric-page-text {
  padding-right: 15rpx;
  vertical-align: middle;
}

.qr-code {
  width: 100%;
  background-color: white;
  text-align: center;
}

.rollMessage {
  height: 60rpx;
  width: 100%;
  border-bottom: 1px dashed #e0e0e0;
  font-size: 24rpx;
  color: #b1b1b1;
  text-indent: 30rpx;
  line-height: 60rpx;
}

.coding-code {
  display: inline-block;
  background-color: #f5f5fa;
  font-size: 28rpx;
  color: #555;
  text-align: center;
  letter-spacing: 2px;
  /* margin-left: 27%; */
  padding: 0 34rpx;
  box-sizing: border-box;
}

.coding-code.show {
  display: block;
}

.coding-code.hide {
  display: none;
}

.coding-time {
  font-size: 22rpx;
  color: #b1b1b1;
  padding: 16rpx 0 30rpx;
}

.codingText {
  padding-left: 30rpx;
}

.numberFeald {
  padding-left: 20rpx;
}

.codingImages {
  width: 400rpx;
  height: 400rpx;
  margin-top: 10rpx;
}

.codingeven-more {
  width: 100%;
  height: 60rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  font-size: 24rpx;
  color: #b1b1b1;
  /* letter-spacing: 3px; */
}

.veent-more-img, .veent-more-img-top {
  width: 22rpx;
  height: 12rpx;
  padding-left: 10rpx;
  vertical-align: middle;
}

.veent-more-img-top {
  transform: rotate(180deg);
}

.qr-code-matter {
  padding-left: 20rpx;
  letter-spacing: 1px;
}

.order-for-shop {
  width: 100%;
  height: 122rpx;
  background-color: #fff;
  display: flex;
  justify-content: center;
  margin-bottom: 20rpx;
}

.order-for-goods {
  width: 100%;
  background-color: white;
  border-top: 1rpx dotted #e0e0e0;
  position: relative;
  padding: 20rpx 36rpx 0 32rpx;
  box-sizing: border-box;
}

.order-for-goods::before, .order-for-goods::after {
  content: '';
  width: 30rpx;
  height: 30rpx;
  border-radius: 50%;
  position: absolute;
  background-color: #f5f5fa;
  top: -15rpx;
}

.order-for-goods::before {
  left: -12rpx;
}

.order-for-goods::after {
  right: -12rpx;
}

.order-for-nei {
  width: 100%;
  padding-bottom: 30rpx;
  /* border-bottom: 1rpx solid #E0E0E0; */
}

.order-for-source {
  display: flex;
  justify-content: space-between;
  width: 100%;
  height: 90rpx;
  line-height: 90rpx;
  color: #606060;
  font-size: 28rpx;
}

.source-r image {
  width: 12rpx;
  height: 22rpx;
  vertical-align: middle;
  margin-left: 18rpx;
}

.order-for-text {
  color: #b1b1b1;
  font-size: 24rpx;
  height: 62rpx;
  border-bottom: 1px dotted #e0e0e0;
  line-height: 62rpx;
}

.order-for-colour {
  width: 100%;
  color: #555;
  font-size: 24rpx;
  height: 56rpx;
  line-height: 56rpx;
  display: flex;
  justify-content: space-between;
  letter-spacing: 4rpx;
}

.warm-prompt {
  width: 100%;
  height: 100rpx;
  color: #b1b1b1;
  font-size: 22rpx;
  display: flex;
  justify-content: center;
  align-items: center;
}

.close {
  width: 22rpx;
  height: 22rpx;
  margin-top: 29rpx;
  margin-left: 29rpx;
}

.motitle {
  width: 100%;
  height: 50rpx;
  line-height: 50rpx;
  margin-top: 50rpx;
  color: #fff;
  font-size: 40rpx;
  text-align: center;
}

.money {
  width: 100%;
  height: 70rpx;
  line-height: 70rpx;
  color: #fff;
  font-size: 28rpx;
  text-align: center;
}

.wallet {
  width: 100%;
  height: 100rpx;
  line-height: 100rpx;
  color: #fff;
  font-size: 24rpx;
  text-align: center;
}

.wallet image {
  width: 19rpx;
  height: 11rpx;
}

.envelope {
  width: 390rpx;
  height: 522rpx;
  background: linear-gradient(to right, #fd4d58, #fc7643);
  position: absolute;
  left: 50%;
  top: 50%;
  margin-top: -308rpx;
  margin-left: -195rpx;
  border-radius: 15rpx;
}

.flod {
  width: 18rpx;
  height: 30rpx;
  margin-left: 183rpx;
  text-align: center;
  margin-bottom: 15rpx;
}

.flod image {
  width: 30rpx;
  height: 100%;
}

.caikai {
  width: 100%;
  height: 23rpx;
  margin: 7rpx 0;
  text-align: center;
  color: #ffc0b9;
  font-size: 24rpx;
}

.received {
  color: #ecf862;
}

.big {
  font-size: 68rpx;
  color: #ecf862;
}

.currency {
  width: 126rpx;
  height: 126rpx;
  line-height: 126rpx;
  margin-top: 50rpx;
  margin-left: 135rpx;
}

.wallet image {
  width: 15rpx;
  height: 19rpx;
  margin-left: 5rpx;
}

.xuanz {
  animation: myfirst 2s;
}

@keyframes myfirst {
  from {
    transform: rotateY(0deg);
  }

  to {
    transform: rotateY(360deg);
  }
}

