.navbar {
  width: 100%;
  height: 80rpx;
  display: flex;
  background-color: #fff;
  position: fixed;
  top: 0rpx;
  left: 0rpx;
  z-index: 99;
  font-size: 24rpx;
  color: rgba(128, 128, 128, 1);
  justify-content: center;
  border-top: 1px solid #e0e0e0;
  align-items: center;
  letter-spacing: 2rpx;
}

.navbar .item {
  width: 375rpx;
  height: 80rpx;
  position: relative;
  text-align: center;
  line-height: 80rpx;
  letter-spacing: 3rpx;
  background: rgba(255, 174, 149, 1);
  color: #fff;
  border-radius: 0;
}

.navbar .item.active {
  color: #fff;
  background: rgba(252, 94, 45, 1);
}

.navbar .item.active:after {
  content: "";
  display: block;
  position: absolute;
  bottom: 0;
  left: 0;
  right: 0;
  height: 4rpx;
}

