  height: 56rpx;
  display: flex;
  justify-content: center;
}

.practicalPayAmount {
  width: 92%;
  height: 56rpx;
  display: flex;
  justify-content: flex-end;
  align-items: center;
  letter-spacing: 2rpx;
}

.practicalPayAmount text:nth-child(1) {
  font-size: 22rpx;
  color: rgba(25, 25, 25, 1);
  font-weight: 600;
}

.practicalPayAmount text:nth-child(2) {
  font-size: 24rpx;
  color: rgba(255, 67, 67, 1);
}

.paymentMagic {
  width: 100%;
  height: 350rpx;
  margin-top: 20rpx;
  letter-spacing: 2rpx;
  background-color: #fff;
}

.paymentHead {
  width: 100%;
  height: 62rpx;
  display: flex;
  justify-content: center;
  border-bottom: 1px dotted #e0e0e0;
  margin-bottom: 25rpx;
}

.paymentHeadkd {
  border: none;
}

.paymentHead-center {
