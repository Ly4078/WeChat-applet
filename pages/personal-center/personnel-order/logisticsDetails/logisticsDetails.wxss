  height: 100%;
  background-color: #fff;
  margin-top: 20rpx;
}

.handiseTop {
  width: 100%;
  height: 176rpx;
  display: flex;
  justify-content: space-between;
  border-bottom: 1px solid #f5f5fa;
}

.handiseTop-left {
  width: 24%;
  height: auto;
  display: flex;
  align-items: center;
}

.handiseTop-right {
  width: 76%;
  height: auto;
  letter-spacing: 2rpx;
}

.handiseTop-img {
  width: 120rpx;
  height: 120rpx;
  background: rgba(255, 255, 255, 1);
  border: 1px solid rgba(235, 235, 235, 1);
  margin-left: 28rpx;
}

.handiseTop-rightText {
  width: 90%;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
  font-size: 28rpx;
  margin-top: 26rpx;
}

.handiseTop-rightCation {
  width: 90%;
  font-size: 20rpx;
  color: rgba(128, 128, 128, 1);
  margin-top: 16rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}

.handiseTop-rightBtn {
  width: 100%;
  display: flex;
  justify-content: space-between;
  margin-top: 12rpx;
}

.handiseTop-rightBtn text:nth-child(2) {
  font-size: 24rpx;
  padding-right: 28rpx;
}

.handiseTop-rightBtn text:nth-child(1) {
  font-size: 28rpx;
  color: rgba(255, 67, 67, 1);
}

.endAmount {
