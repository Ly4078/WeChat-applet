page{
  background-color: #F5F5FA;
}

.logisticsTop{
  width: 100%;
  height: 150rpx;
  background: -webkit-linear-gradient(left, #fd8621 , #faaf39);
  position: relative;
}

.logisticsTopImg{
  width: 100%;
  height: 150rpx;
  position: absolute;
  left: 0rpx;
  top: 0rpx;
}

.logisticsTopText{
  width: 100%;
  height: 150rpx;
  position: absolute;
  left: 0rpx;
  top: 0rpx;
  font-size: 32rpx;
  color:rgba(255,255,255,1);
  letter-spacing: 3rpx;
  line-height: 150rpx;
}

.logisticsTopShould{
  width: 100%;
  height: 150rpx;
  position: absolute;
  left: 0rpx;
  top: 0rpx;
  font-size: 32rpx;
  color:rgba(255,255,255,1);
  letter-spacing: 3rpx;
  line-height: 54rpx;
}

.logisticsTopfront{
  margin-top:22rpx;
  margin-left: 32rpx;
}

.logisticsTopCount{
  margin-left: 32rpx;
  font-size: 23rpx;
}

.logisticsTopText text{
  margin-left: 32rpx;
}

.expressage{
  width: 100%;
  height: 126rpx;
  background-color: #FFFFFF;
  display: flex;
  justify-content: space-between;
  align-items: center;
  margin-top: 20rpx;
}

.expressageImg{
  width: 40rpx;
  height: 34rpx;
  margin-left: 28rpx;
}

.expressageText{
  width: 80%;
  height: auto;
}

.expressageSite{
  width: 90%;
  overflow: hidden;
  text-overflow:ellipsis; 
  white-space: nowrap;
  color:rgba(252,94,45,1);
  font-size: 28rpx;
}

.expressageTime{
  font-size:24rpx;
  color:rgba(128,128,128,1);
  padding-top: 16rpx;
}

.siteImg{
  width: 40rpx;
  height: 34rpx;
  margin-left: 28rpx;
}

.yourself{
  width: 88%;
}

.expressageSiteName{
  font-size:30rpx;
  color:rgba(25,25,25,1);
}

.expressageSiteName text{
  font-size:24rpx;
  color:rgba(25,25,25,1);
  margin-left: 45rpx;
}

.expressageMsg{
  width: 88%;
  font-size:24rpx;
  color:#191919;
  overflow: hidden;
  text-overflow:ellipsis; 
  white-space: nowrap;
  padding-top: 16rpx;
}

.handise{
  width: 100%;
  height: 100%;
  background-color: #FFFFFF;
  margin-top: 20rpx;
}

.handiseTop{
  width: 100%;
  height: 176rpx;
  display: flex;
  justify-content: space-between;
  border-bottom: 1px solid #F5F5FA;
}

.handiseTop-left{
  width: 24%;
  height: auto;
  display: flex;
  align-items: center;
}

.handiseTop-right{
  width: 76%;
  height: auto;
  letter-spacing: 2rpx;
}

.handiseTop-img{
  width: 120rpx;
  height: 120rpx;
  background:rgba(255,255,255,1);
  border:1px solid rgba(235,235,235,1);
  margin-left: 28rpx;
}

.handiseTop-rightText{
  width: 90%;
  overflow: hidden;
  text-overflow:ellipsis;
  white-space: nowrap;
  font-size:28rpx;
  margin-top: 26rpx;
}

.handiseTop-rightCation{
  width: 90%;
  font-size:20rpx;
  color:rgba(128,128,128,1);
  margin-top: 16rpx;
  overflow: hidden;
  text-overflow:ellipsis;
  white-space: nowrap;
}

.handiseTop-rightBtn{
  width: 100%;
  display: flex;
  justify-content: space-between;
  margin-top: 12rpx;
}

.handiseTop-rightBtn text:nth-child(2){
  font-size:24rpx;
  color:rgba(25,25,25,1);
  padding-right: 28rpx;
}

.handiseTop-rightBtn text:nth-child(1){
  font-size:28rpx;
  color:rgba(255,67,67,1);
}

.endAmount{
  width: 100%;
  font-size: 22rpx;
  display: flex;
  justify-content: center;
  padding-top: 20rpx;
}

.endAmount:nth-child(4){
  border-bottom: 1px solid #F5F5FA;
  padding-bottom: 20rpx;
}

.endAmountNei{
  width: 92%;
  display: flex;
  justify-content: space-between;
}

.practicalPay{
  width: 100%;
  height: 56rpx;
  display: flex;
  justify-content: center;
}

.practicalPayAmount{
  width: 92%;
  height: 56rpx;
  display: flex;
  justify-content: flex-end;
  align-items: center;
  letter-spacing: 2rpx;
}

.practicalPayAmount text:nth-child(1){
  font-size:22rpx;
  color:rgba(25,25,25,1);
  font-weight: 600;
}

.practicalPayAmount text:nth-child(2){
  font-size:24rpx;
  color:rgba(255,67,67,1);
}

.paymentMagic{
  width: 100%;
  height: 286rpx;
  margin-top: 20rpx;
  letter-spacing: 2rpx;
  background-color: #FFFFFF;
}

.paymentHead{
  width: 100%;
  height: 62rpx;
  display: flex;
  justify-content: center;
  border-bottom: 1px dotted #E0E0E0;
}

.paymentHead-center{
  width: 92%;
  height: 62rpx;
  font-size: 24rpx;
  color:rgba(177,177,177,1);
  font-weight: 600;
  display: flex;
  align-items: center;
}

.payment-execute{
  width: 100%;
  height: 222rpx;
  padding-bottom:  140rpx;
}

.payment-copy{
  width: 100%;
  display: flex;
  justify-content: space-between;
  align-items: center;
  padding-top: 10rpx;
}

.payment-copyText{
  width: 60%;
  font-size:22rpx;
  color:rgba(85,85,85,1);
  margin-left: 32rpx;
}

.payment-copyText text:nth-child(2){
  padding-left: 106rpx;
}

.payment-copyRight{
  width: 40%;
  display: flex;
  justify-content: flex-end;
}

.payment-copyRight button{
  width: 120rpx;
  height: 60rpx;
  font-size:22rpx;
  color:rgba(252,94,45,1);
  background-color: #FFFFFF;
}

.inconformity{
  width: 100%;
  font-size:22rpx;
  color:rgba(85,85,85,1);
  margin-bottom: 24rpx;
}

.inconformity text:nth-child(1){
    margin-left: 32rpx;
}

.inconformity text:nth-child(2){
  padding-left: 106rpx;
}

.secondOrder{
  width: 100%;
  height: 100rpx;
  background-color: #FFFFFF;
  position: fixed;
  bottom: 0rpx;
  left: 0rpx;
  z-index: 9999;
}

.secondOrderLeader{
  width: 100%;
  display: flex;
  justify-content: flex-end;
  position: fixed;
  bottom: 0rpx;
  left: 0rpx;
  background-color: #FFFFFF;
  height: 100rpx;
  align-items: center;
}

.secondOrderLeader button{
  width:158rpx;
  height:56rpx;
  border:2rpx solid rgba(177,177,177,1);
  border-radius:28rpx;
  font-size: 28rpx;
  color:rgba(177,177,177,1);
  text-align: center;
  line-height: 56rpx;
  letter-spacing: 2rpx;
  margin-right: 28rpx;
}

.secondOrderLeader button:nth-child(2){
  color: #FFFFFF;
  background-color: #FC6232;
  border: none;
}