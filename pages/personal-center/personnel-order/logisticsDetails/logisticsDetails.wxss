  height: 60rpx;
  font-size: 22rpx;
  color: rgba(252, 94, 45, 1);
  background-color: #fff;
}

.inconformity {
  width: 100%;
  font-size: 22rpx;
  color: rgba(85, 85, 85, 1);
  margin-bottom: 24rpx;
}

.inconformity text:nth-child(1) {
  margin-left: 32rpx;
}

.inconformity text:nth-child(2) {
  padding-left: 106rpx;
}

.secondOrder {
  width: 100%;
  height: 100rpx;
  background-color: #fff;
  position: fixed;
  bottom: 0rpx;
  left: 0rpx;
  z-index: 9999;
}

.secondOrderLeader {
  width: 100%;
  display: flex;
  justify-content: flex-end;
  position: fixed;
  bottom: 0rpx;
  left: 0rpx;
  background-color: #fff;
  height: 100rpx;
  align-items: center;
}

.secondOrderLeader button {
  width: 158rpx;
  height: 56rpx;
  border: 2rpx solid rgba(177, 177, 177, 1);
  border-radius: 28rpx;
  font-size: 28rpx;
  color: rgba(177, 177, 177, 1);
  text-align: center;
  line-height: 56rpx;
  letter-spacing: 2rpx;
  margin-right: 28rpx;
}

.secondOrderLeader button:last-child {
  color: #fff;
  background-color: #fc6232;
  border: none;
}

.cheng {
  color: #fff !important;
  background: #fc6232;
  border: none !important;
}

.Refund {
  font-size: 30rpx;
  color: #969696 !important;
  flex: 1;
  margin: 0 20rpx;
  background: none !important;
  border: none !important;
}

/* 订单详情，参团部分 */

.Single {
  display: flex;
  flex-direction: column;
  margin-top: 30rpx;
}

.Single-title {
  padding: 20rpx 30rpx;
  display: flex;
  background: #fc6232;
  flex-direction: row;
  align-items: center;
}

.Single-title .left {
  flex: 1;
}

.Single-title view, .Single-title text {
  color: #fff;
  font-size: 28rpx;
}

.Single-detail {
  padding: 20rpx 30rpx;
  background: #fff;
  display: flex;
  flex-direction: column;
  border-bottom:1rpx solid #EBEBEB;
}

.Single-detail-title {
  display: flex;
  font-size: 26rpx;
  flex-direction: row;
}

.Single-detail-title .Single-detail-title-left {
  display: flex;
  flex-direction: row;
  flex:1;
  color:#191919;
}
.Single-detail-title-time{
  color:#969696;
}

.Single-detail-info{
  display: flex;
  flex-direction: row;
  align-items: center;
  padding: 20rpx 0 0 0;

}
.Single-detail-info .Single-detail-info-friend{
  flex:1;
}
.Single-detail-info .Single-detail-info-friend image{
  width: 90rpx;
  height: 90rpx;
  border-radius: 50%;
  margin-right:20rpx;
}
.Single-detail-info .Single-detail-info-share{
  background: #FC6232;
  color:#fff;
  font-size: 32rpx;
  padding:10rpx 20rpx;
  margin:0;
  line-height: 1.5;
  border-radius: 40rpx;
}
.Single-detail-info-share::before,Single-detail-info-share::after{
  padding: 0;
  margin: 0;
  border:none;
}
.Single-progress{
  background: #fff;
  padding:40rpx 30rpx;
  position: relative;
}
.Single-progress .content{
  display: flex;
  position: absolute;
  left: 0;
  top: 0;
  right:0;
  bottom:0;
  flex-direction: row;
  font-size: 28rpx;
  z-index: 22;
  justify-content: space-around;
  align-items: center;
}
.Single-progress .progress{
  position: absolute;
  width: 1%;
  left: 0;
  top: 0;
  height: 100%;
  background: #FFE4DB;
  z-index: 1;
  transition: all .4s;
}
