.particulars{
  width: 100%;
  height: 110rpx;
  border-bottom: 1px solid #E0E0E0;
  display: flex;
  justify-content: space-between;
  align-items: center;
  letter-spacing: 2rpx;
}

.particulars_left{
  width: 74%;
  padding-left: 26rpx;
  line-height: 44rpx;
}

.particulars_left text:nth-child(1){
  font-size: 28rpx;
  color: #191919;
}

.particulars_left text:nth-child(2){
  font-size: 22rpx;
  color: #808080;
  display: block;
}

.particulars_right{
  width: 26%;
  text-align: right;
  padding-right: 28rpx;
  color: #FC5E2D;
}