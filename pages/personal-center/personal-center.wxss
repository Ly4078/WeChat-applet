page{
  background-color: #F5F5FA;
  width: 100%;
  height: 100%;
}
.personal_Top {
  height: 350rpx;
  background: linear-gradient(to bottom, #FC8E2D, #FC5E2D);
}
.centreWrap {
  display: flex;
  justify-content: space-between;
  height: 176rpx;
  /* border-bottom: 1rpx solid #ED5E15; */
}

.cs_button{
  width: 100%;
  height: 100%;
  position: absolute;
  border: 1px solid red;
  opacity: 0;
  top: 0;
  left: 0;
}

.theNameof{
  width: 100%;
  height: 100%;
  margin-left: 28rpx;
  display: flex;
  justify-content: flex-start;
  align-items: center;
}

.centreImages {
  width: 110rpx;
  height: 110rpx;
  border-radius: 50%;
  /* border: 1px solid #e0e0e0; */
  /* margin-top: -1rpx; */
  /* margin-left: -1rpx; */
  z-index: 3;
}
.suerinfo{
  width: 110rpx;
  height: 110rpx;
  border-radius: 50%;
  margin: 0rpx 30rpx 0rpx 20rpx;
}
.suerinfo::after{
 border: none;
}

.TopRight{
  width: 21%;
  position: relative;
  z-index: 100;
  overflow: hidden;
}

.TopRight button{
  width: 166rpx;
  height: 50rpx;
  margin-top: 64rpx;
  padding-right: 30rpx;
  background:rgba(255,255,255,1);
  opacity: 0.94;
  box-shadow:2px 0px 6px rgba(164,68,1,0.54);
  font-size: 28rpx;
  text-align: center;
  line-height: 50rpx;
  border-top-left-radius: 28rpx;
  border-bottom-left-radius: 28rpx;
  color: #FC832D;
  letter-spacing: 2rpx;
}

.centreText {
  color: #fff;
  width: 366rpx;
  overflow: hidden;
  font-size: 36rpx;
  font-weight: 600;
  font-family: Adobe Heiti Std R;
}

.variances{
  display: flex;
  padding-top: 20rpx;
}

.messageTop {
  display: block;
  letter-spacing: 4rpx;
  font-weight: 600;
  color: #fff;
  margin-bottom:10rpx;
}

.messageWrap {
  flex-grow: 1;
  text-align: center;
  font-size: 26rpx;
  position: relative;
  color: #fff;
}


.messageWrap button ,.botton-hover{
  background: none;
  color: none;
}
.messageWrap:nth-child(1){
  width: 8%;
}

.messageWrap:nth-child(1) form .messageTop{
  margin-top: -18rpx;
}
.messageWrap image {
  height: 56rpx;
  width: 56rpx;
}

