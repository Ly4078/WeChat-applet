.shadowBox {
  position: fixed;
  width: 100%;
  height: 100%;
  background-color: rgba(0,0,0,0.7);
  z-index: 1000;
  display: flex;
  justify-content: center;
  align-items: center;
}

.shadow_inner {
  width: 422rpx;
  height: 308rpx;
  background-color: #fff;
  border-radius: 16rpx;
  opacity: 1;
  padding: 44rpx 52rpx 0 52rpx;
  box-sizing: border-box;
  text-align: center;
}

.shadow_list {
  height: 74rpx;
  line-height: 74rpx;
  margin-bottom: 26rpx;
  border: 1rpx solid #EBEBEB;
  border-radius: 10rpx;
  font-size: 26rpx;
}

.shadow_list.onlineCall {
  position: relative;
}

.shadow_list image {
  vertical-align: middle;
  margin-right: 28rpx;
}

.shadow_list text {
  vertical-align: middle;
}

.shadow_time {
  font-size: 22rpx;
}